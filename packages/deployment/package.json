{
  "name": "@cumulus/deployment",
  "version": "1.19.0",
  "description": "Deployment templates for cumulus",
  "bin": {
    "delete-stage": "bin/delete-stage-cli.js"
  },
  "scripts": {
    "test": "ava",
    "test-coverage": "nyc npm test",
    "build": "babel lib -d app",
    "watch": "babel lib -d app -w",
    "prepare": "npm run build"
  },
  "ava": {
    "files": "test"
  },
  "engines": {
    "node": ">=10.16.3"
  },
  "keywords": [
    "CUMULUS"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus"
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "babel": {
    "presets": [
      "es2017"
    ]
  },
  "homepage": "https://github.com/nasa/cumulus/tree/master/packages/deployment",
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/common": "1.19.0",
    "aws-sdk": "^2.585.0",
    "commander": "^2.20.0",
    "extract-zip": "^1.6.6",
    "fs-extra": "^5.0.0",
    "got": "^9.2.1",
    "handlebars": "^4.0.11",
    "kes": "~2.2.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.omit": "^4.5.0",
    "lodash.zipobject": "^4.1.3",
    "node-forge": "^0.7.1",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@cumulus/aws-client": "1.19.0",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2017": "^6.24.1",
    "nock": "10.0.1",
    "nyc": "^14.0.0",
    "sinon": "^7.1.1"
  }
}
